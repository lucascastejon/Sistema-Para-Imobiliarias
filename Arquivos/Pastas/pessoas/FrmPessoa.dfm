  object ZqInteressados: TZQuery
    Connection = PRINCIPAL.Conexao
    SQL.Strings = (
      
        'select PI.id_conta as id_pessoa, pi.componente as nome, pi.data ' +
        'as data, '#39'('#39'||it.nome||'#39') '#39'||l.nome||'#39' '#39'||COALESCE(i.complemento' +
        ', '#39#39')||'#39', '#39'||i.NUMERO||'#39', '#39'||b.nome||'#39' de '#39'||i.DORMITORIOS||'#39' qu' +
        'artos em '#39'||c.nome||'#39' (R$'#39'||I.valor_aluquel||'#39')-'#39'||i.ID_IMOVEL a' +
        's obs, PI.id_log as id_pessoa_imovel'
      'from log pi'
      'left join imovel i on i.id_imovel = pi.id_conta'
      'left join logradouro L ON L.id_logradouro = I.id_logradouro'
      'left JOIN cidade C ON C.id_cidade = L.id_cidade'
      'left JOIN bairro B ON B.id_bairro = L.id_bairro'
      'left join imovel_tipo it on it.id_imovel_tipo = i.id_imovel_tipo'
      
        'where pi.acao = '#39'CHV'#39' and pi.ativo = '#39'SIM'#39' and (pi.obs like :pcp' +
        'f or pi.obs like :prg) order by pi.data desc')
    Params = <
      item
        DataType = ftUnknown
        Name = 'pcpf'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'prg'
        ParamType = ptUnknown
      end>
    Left = 776
    Top = 8
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'pcpf'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'prg'
        ParamType = ptUnknown
      end>
  end
