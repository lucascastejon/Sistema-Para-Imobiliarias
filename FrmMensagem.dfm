  object ZSql: TZQuery
    Connection = PRINCIPAL.Conexao
    UpdateObject = ZUpdate
    SQL.Strings = (
      'select l.*, r.nome as remetente,'
      'case l.componente'
      '   when '#39'FUNCIONARIO'#39' then'
      '   (select nome from usuario where id_usuario = l.id_conta)'
      '   when '#39'PESSOA'#39' then'
      '   (select nome from pessoa where id_pessoa = l.id_conta)'
      '   else'
      '       '#39'N'#195'O INFORMADO'#39
      'end as destinatario'
      'from log l'
      'inner join pessoa r on r.id_pessoa = l.id_func'
      
        'where l.ativo = '#39'SIM'#39' and l.acao = '#39'MSN'#39' and l.componente = '#39'MEN' +
        'SAGEM'#39
      'order by l.data')
    Params = <>
    Left = 448
    Top = 136
  end
